    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GIP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ISR">
            <Value>=Parameters!ISR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ISR, MasterBranchCustomerNbr, Company, Goal, ProgramRevenue as Achievement, PotentialPayout = ExpPayout, CurrentPayout 
FROM gip.dbo.GIP_OUTPUT 
WHERE program = 'Lenovo ISR' AND (ISR IN (@ISR))
Order by ProgramRevenue desc, Company</CommandText>
      </Query>
